psql/output/ <- [-timecheck method:psql_schema]

psql/output/acs <- output/acs.py, psql/input/acs
    $INPUT && touch $OUTPUT

psql/output/kids <- output/kids.sql, psql/aux/kids, psql/output/tests, psql/aux/kid_wics [method:psql]

; join tests with kid_tests and test_addresses to get all the ids in one place
psql/output/tests <- output/tests.sql, psql/aux/tests, psql/aux/kid_tests_info, psql/aux/test_addresses [method:psql]

psql/output/kid_addresses <- output/kid_addresses.sql, psql/aux/kid_wic_addresses, psql/output/tests [method:psql]

; residential flag calculated by combination of data
; including tests, wic, assessor, inspections
psql/output/addresses <- output/addresses.sql, psql/aux/addresses, psql/output/inspections, psql/aux/complex_addresses, psql/aux/assessor, psql/output/kid_addresses [method:psql]

psql/output/inspections <- output/inspections.sql, psql/aux/addresses, psql/aux/inspections, psql/aux/inspection_addresses [method:psql]
