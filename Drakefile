psql()
    psql -v ON_ERROR_STOP=1 -f $INPUT 2>&1 && touch $OUTPUT

; create a schema (does not drop)
psql_schema()
    schema=$(basename $OUTPUT)
    psql -v ON_ERROR_STOP=1 -c "CREATE SCHEMA IF NOT EXISTS $schema" && mkdir -p $OUTPUT && touch $OUTPUT/schema

%include default_profile

