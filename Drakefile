BASE:=.

psql()
    psql -v ON_ERROR_STOP=1 -f $INPUT && touch $OUTPUT

psql_schema()
    schema=$(basename $OUTPUT)
    psql -v ON_ERROR_STOP=1 -c "DROP SCHEMA IF EXISTS $schema CASCADE; CREATE SCHEMA $schema" && mkdir -p $OUTPUT && touch $OUTPUT/schema

pickle_table()
	TABLENAME=$(echo $INPUT | sed 's/.*psql\///;s/\//./')
        $[BASE]/output/pickle_table.py $TABLENAME $OUTPUT

PROFILE:=/home/epotash/lead/default_profile
ACS_PROFILE:=/home/epotash/lead/acs_profile

%include $[PROFILE]
PYTHONPATH=/home/epotash/

%include $[BASE]/input/Drakefile

%include $[BASE]/wic/Drakefile

%include $[BASE]/buildings/Drakefile

%include $[BASE]/aux/dedupe/Drakefile

%include $[BASE]/aux/Drakefile

%include $[BASE]/output/Drakefile
