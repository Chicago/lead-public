BASE:=.

psql()
    psql -v ON_ERROR_STOP=1 -f $INPUT && touch $OUTPUT

; create a schema (does not drop)
psql_schema()
    schema=$(basename $OUTPUT)
    psql -v ON_ERROR_STOP=1 -c "CREATE SCHEMA IF NOT EXISTS $schema" && mkdir -p $OUTPUT

pickle_table()
	TABLENAME=$(echo $INPUT | sed 's/.*psql\///;s/\//./')
        $[BASE]/output/pickle_table.py $TABLENAME $OUTPUT

PROFILE:=/home/epotash/lead/default_profile
ACS_PROFILE:=/home/epotash/lead/acs_profile

%include $[PROFILE]
PYTHONPATH=/home/epotash/

%include input/Drakefile

%include wic/Drakefile

%include buildings/Drakefile

%include aux/dedupe/Drakefile

%include aux/Drakefile

%include output/Drakefile
