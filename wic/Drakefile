data/wic/pdftotext/ <- [-timecheck]
    mkdir -p $OUTPUT
    for filename in $WIC_DIR/*.pdf; do
        pdftotext -layout "$filename" $OUTPUT/$(basename "$filename" .pdf).txt
    done

data/wic/grep/ <- data/wic/pdftotext/
    mkdir -p $OUTPUT
    rm $OUTPUT/*.txt
    for filename in $INPUT/*_I.txt; do
        clinic=$(basename "$filename" .txt | sed 's/_I$//')
        # remove header and formatting lines from pdf output
        grep -v $'\(^$\|Program\|report\|Request\|Infants\|Filter\|Records Selected\|GRAND TOTAL\|Printed\|^\f\)' "$filename" > $OUTPUT/$(basename "$filename")
    done

data/wic/csv/ <- data/wic/grep/, wic/to_csv.sh, wic/to_csv.py
    $INPUT1 

psql/input/wic <- data/wic/csv/
    psql -c 'DROP TABLE IF EXISTS input.wic'
    psql -c 'CREATE TABLE input.wic (first_name text, last_name text, address text, zip int, date_of_birth date, household_income int, household_size int, assistance1 int, assistance2 int, assistance3 int, assistance4 int, assistance5 int, clinic text);'
    for filename in $INPUT/*_I.txt; do
        echo $filename
        psql -c "\copy input.wic from '"$filename"' with csv header"
    done
    touch $OUTPUT

