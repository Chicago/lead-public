$[SQL_DIR]/discontinuity/schema <- [method:psql_schema -timecheck]

$[SQL_DIR]/discontinuity/max_bll_under1 <- discontinuity/max_bll_under1.sql, $[SQL_DIR]/discontinuity/schema, $[SQL_DIR]/output/kids_extra, $[SQL_DIR]/output/kids [method:psql]

; addresses for kid before first_sample_date
$[SQL_DIR]/discontinuity/addresses <- discontinuity/addresses.sql, $[SQL_DIR]/discontinuity/max_bll_under1, $[SQL_DIR]/output/kids, $[SQL_DIR]/output/kid_addresses [method:psql]

; past tests at first_sample_address
$[SQL_DIR]/discontinuity/tests <- discontinuity/tests.sql, $[SQL_DIR]/discontinuity/max_bll_under1, $[SQL_DIR]/output/tests, $[SQL_DIR]/discontinuity/addresses [method:psql]

; past investigations at first_sample_address
$[SQL_DIR]/discontinuity/investigations <- discontinuity/investigations.sql, $[SQL_DIR]/discontinuity/max_bll_under1, $[SQL_DIR]/output/investigations, $[SQL_DIR]/discontinuity/addresses [method:psql]

; buildings info for first_sample_address
$[SQL_DIR]/discontinuity/buildings <- discontinuity/buildings.sql, $[SQL_DIR]/discontinuity/max_bll_under1, $[SQL_DIR]/output/kids, $[SQL_DIR]/output/addresses, $[SQL_DIR]/aux/buildings, $[SQL_DIR]/aux/assessor, $[SQL_DIR]/discontinuity/addresses [method:psql]

; past kids as first_sample_address
$[SQL_DIR]/discontinuity/kids <- discontinuity/kids.sql, $[SQL_DIR]/discontinuity/max_bll_under1, $[SQL_DIR]/output/kids, $[SQL_DIR]/output/kid_addresses, $[SQL_DIR]/output/tests, $[SQL_DIR]/discontinuity/addresses [method:psql]

; labs on first_sample_date
$[SQL_DIR]/discontinuity/labs <- discontinuity/labs.sql, $[SQL_DIR]/discontinuity/max_bll_under1, $[SQL_DIR]/output/kids, $[SQL_DIR]/output/tests [method:psql]
